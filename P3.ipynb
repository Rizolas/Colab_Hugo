      "source": [
        "class area():\n",
        "\n",
        "  def datos (self):\n",
        "    self.h = float(input())\n",
        "    self.b = float(input())\n",
        "\n",
        "  def area_triangulo(self):\n",
        "    return (self.h * self.b)/2\n",
        "\n",
        "  def pro (self):\n",
        "    self.datos()\n",
        "    A = self.area_triangulo()\n",
        "    print(f\"El area del triangulo es: {A}\")\n",
        "\n",
        "ar = area()\n",
        "ar.pro()"
